              <styleClass>
                  <String fx:value="text-field-size-20"/>
                  <String fx:value="jfx-text-field-white-text"/>
              </styleClass>
            </JFXTextField>
            <ImageView fitHeight="35.0" fitWidth="33.0" pickOnBounds="true" preserveRatio="true">
              <image>
                  <Image url="@../images/PhoneNumber.png"/>
              </image>
            </ImageView>
          </children>
        </HBox>
      </children>
    </HBox>
      <Region layoutX="40.0" layoutY="526.0" maxHeight="-Infinity" maxWidth="1.7976931348623157E308"
              minHeight="-Infinity" minWidth="-Infinity" prefHeight="60.0"/>
      <HBox alignment="CENTER" layoutX="40.0" layoutY="429.0" maxHeight="-Infinity" maxWidth="1.7976931348623157E308"
            minHeight="-Infinity">
      <children>
          <JFXButton fx:id="backToStep1BTN" maxWidth="-Infinity" prefHeight="80.0" prefWidth="160.0"
                     ripplerFill="#9bc7fa" styleClass="sign-in-button" text="Back" HBox.hgrow="ALWAYS">
          <font>
              <Font name="System Bold" size="16.0"/>
          </font>
        </JFXButton>
          <Region maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity"
