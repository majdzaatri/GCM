                          <styleClass>
                              <String fx:value="transparent"/>
                              <String fx:value="text-field-size-20"/>
                              <String fx:value="jfx-text-field-white-text"/>
                          </styleClass>
                      </JFXTextField>
                      <ImageView fitHeight="35.0" fitWidth="33.0" pickOnBounds="true" preserveRatio="true">
                          <image>
                              <Image url="@../images/icons8-name-tag-64.png"/>
                          </image>
                      </ImageView>
                  </children>
                  <opaqueInsets>
                      <Insets/>
                  </opaqueInsets>
              </HBox>
          </children>
      </HBox>
      <Region maxHeight="-Infinity" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity"
              prefHeight="60.0"/>
      <HBox fx:id="usernameHB" alignment="CENTER_LEFT" styleClass="hbox-sign-in-form">
      <children>
          <JFXTextField fx:id="usernameTF" focusColor="#fa65b3" labelFloat="true" prefHeight="31.0" prefWidth="352.0"
