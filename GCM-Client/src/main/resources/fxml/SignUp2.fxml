                  <children>
                      <JFXTextField fx:id="lastNameTF" focusColor="#fa65b3" labelFloat="true"
                                    maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                                    prefHeight="38.0" prefWidth="160.0" promptText="Last Name" unFocusColor="#a6a6a6"
