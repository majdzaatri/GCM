                  <String fx:value="transparent"/>
                  <String fx:value="text-field-size-20"/>
                  <String fx:value="jfx-text-field-white-text"/>
              </styleClass>
          </JFXTextField>
        <ImageView fitHeight="35.0" fitWidth="33.0" pickOnBounds="true" preserveRatio="true">
          <image>
              <Image url="@../images/username.png"/>
          </image>
        </ImageView>
      </children>
      <opaqueInsets>
          <Insets/>
      </opaqueInsets>
    </HBox>
      <Region maxHeight="-Infinity" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity"
              prefHeight="60.0"/>
    <HBox>
      <children>
          <HBox alignment="CENTER_LEFT" maxHeight="1.7976931348623157E308" maxWidth="-Infinity"
                styleClass="hbox-sign-in-form" HBox.hgrow="ALWAYS">
          <children>
              <JFXComboBox fx:id="phoneNumberPrefixCB" focusColor="#fa65b3" labelFloat="true"
