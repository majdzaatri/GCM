  <children>
    <Label maxHeight="-Infinity" maxWidth="-Infinity" text="Sign Up">
      <VBox.margin>
          <Insets/>
      </VBox.margin>
      <styleClass>
          <String fx:value="label-45"/>
          <String fx:value="colored-label-white"/>
      </styleClass>
    </Label>
      <Region layoutX="40.0" layoutY="370.0" maxHeight="-Infinity" maxWidth="1.7976931348623157E308"
              minHeight="-Infinity" minWidth="-Infinity" prefHeight="70.0"/>
      <HBox alignment="CENTER" prefHeight="40.0" prefWidth="403.0" spacing="20.0">
          <children>
              <HBox fx:id="firstNameHB" alignment="CENTER_LEFT" prefHeight="40.0" prefWidth="170.0"
                    styleClass="hbox-sign-in-form" HBox.hgrow="ALWAYS">
                  <children>
                      <JFXTextField fx:id="firstNameTF" focusColor="#fa65b3" labelFloat="true"
                                    maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                                    prefHeight="38.0" prefWidth="160.0" promptText="First Name" unFocusColor="#a6a6a6"
                                    HBox.hgrow="ALWAYS">
                          <styleClass>
                              <String fx:value="transparent"/>
                              <String fx:value="text-field-size-20"/>
                              <String fx:value="jfx-text-field-white-text"/>
                          </styleClass>
                      </JFXTextField>
                      <ImageView fitHeight="35.0" fitWidth="33.0" pickOnBounds="true" preserveRatio="true">
                          <image>
                              <Image url="@../images/icons8-name-tag-64.png"/>
                          </image>
                      </ImageView>
                  </children>
                  <opaqueInsets>
                      <Insets/>
                  </opaqueInsets>
              </HBox>
              <HBox fx:id="lastNameHB" alignment="CENTER_LEFT" prefWidth="170.0" styleClass="hbox-sign-in-form"
